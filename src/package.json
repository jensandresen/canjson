{
  "name": "canjson",
  "version": "1.0.0",
  "description": "Create a server that serves canned json answers defined in a single configuration file.",
  "main": "index.js",
  "files": [
    "./*.js"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "mocha --recursive ./tests/*.js",
    "test:watch": "mocha --recursive --watch ./tests/*.js",
    "test:run": "node index.js -c ./../test_config.json",
    "mocha": "mocha --help"
  },
  "keywords": ["fake", "stub", "json", "canned", "canned answer", "server"],
  "author": "Jens Andresen <jensandresen@gmail.com>",
  "repository": {
    "url": "https://github.com/jensandresen/canjson",
    "type": "git"
  },
  "license": "MIT",
  "devDependencies": {
    "mocha": "^6.2.0"
  },
  "dependencies": {
    "minimist": "^1.2.0"
  }
}
